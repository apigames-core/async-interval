{
  "name": "set-interval-async",
  "version": "1.0.5",
  "description": "Modern setInterval for async functions with reentrancy protection.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ealmansi/set-interval-async"
  },
  "main": "dist/set-interval-async.cjs.js",
  "module": "dist/set-interval-async.esm.mjs",
  "unpkg": "dist/set-interval-async.iife.js",
  "library": "SetIntervalAsync",
  "files": [
    "dynamic/**",
    "fixed/**",
    "legacy/**",
    "dist/**"
  ],
  "author": "Emilio Almansi",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "jsdoc": "3.5.5",
    "lolex": "3.0.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "rimraf": "2.6.3",
    "rollup": "1.1.0",
    "rollup-plugin-babel": "4.3.0",
    "rollup-plugin-babel-minify": "6.2.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@babel/runtime": "7.2.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "clean": "rimraf set-interval-async.* dynamic fixed legacy .nyc_output docs",
    "docs": "jsdoc -d docs -R README.md src/",
    "lint": "standard src/**/*.js test/**/*.js rollup.config.js",
    "pretest": "yarn lint",
    "test": "nyc mocha --require test/babel.js",
    "prepublishOnly": "yarn clean && yarn build && yarn test && yarn docs && git add docs"
